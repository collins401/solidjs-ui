    "clsx": "^1.2.1",
    "dayjs": "^1.11.9",
    "goober": "^2.1.13",
    "jsdom": "^22.1.0",
    "redaxios": "^0.5.1",
    "solid-js": "^1.7.7",
    "solid-styled-components": "^0.28.5",
